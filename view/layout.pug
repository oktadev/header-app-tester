doctype html
html
  head
    title Header App Tester - #{title}
    link(rel='stylesheet', href='https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css' integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous")
    link(rel='stylesheet', href='/css/1995.css' title="main")
    link(rel='stylesheet', href='https://fonts.googleapis.com/icon?family=Material+Icons')
  body
    .container-fluid
      .row
        .col-md-12
          nav.navbar.navbar-expand-lg.navbar-light.bg-light.navbar-dark.bg-dark
            button.navbar-toggler(type='button', data-toggle='collapse', data-target='#bs-example-navbar-collapse-1')
              span.navbar-toggler-icon
            a.navbar-brand(href='#') Header App Tester
            #bs-example-navbar-collapse-1.collapse.navbar-collapse
              ul.navbar-nav.ml-md-auto
                li.nav-item.active
                  a.nav-link(href='#') Home 
                    span.sr-only (current)
                li.nav-item.dropdown
                  a#navbarTheme.nav-link.dropdown-toggle(href='#', data-toggle='dropdown') Themes
                  .dropdown-menu.dropdown-menu-right(aria-labelledby='navbarTheme')
                    a.dropdown-item(href='#', class='change-style-menu-item', rel='1977') In a galaxy far far away (1977)
                    a.dropdown-item(href='#', class='change-style-menu-item', rel='1980') Mainframo (1985)
                    a.dropdown-item(href='#', class='change-style-menu-item', rel='1995') Look mom, I'm a Web Designer (1995)
                    a.dropdown-item(href='#', class='change-style-menu-item', rel='1999') Applets are the future (1999)
                li.nav-item.dropdown
                  a#navbarDropdownMenuLink.nav-link.dropdown-toggle(href='#', data-toggle='dropdown') Other apps
                  .dropdown-menu.dropdown-menu-right(aria-labelledby='navbarDropdownMenuLink')
                    a.dropdown-item(href='/') Index
                    a.dropdown-item(href='/sample') Header-Based App
                    a.dropdown-item(href='/apex') Oracle APEX
                    a.dropdown-item(href='/weblogic') Oracle Weblogic
                    a.dropdown-item(href='/webseal') IBM WebSeal
                    a.dropdown-item(href='/psft/crm') Oracle PeopleSoft CRM
                    a.dropdown-item(href='/psft/hcm') Oracle PeopleSoft HCM
                    a.dropdown-item(href='/psft/elm') Oracle PeopleSoft E-Learning
                    a.dropdown-item(href='/psft/fscm') Oracle PeopleSoft Financial
                    a.dropdown-item(href='/psft/camp') Oracle PeopleSoft Campus
                    .dropdown-divider
                    a.dropdown-item(href='//'+req.headers["host"]+'/toc.html' target='_toc') Terms of use
                    a.dropdown-item(href='./spgwLogout') Sign-Out
      .row
        .col-md-12
          .jumbotron
            h2 #{title}
            p #{description}
            if doc
              p
                a.btn.btn-primary.btn-large(href=`${doc}`, target="_doc") Learn more
      hr
      .row
        .col-md-12
          ul#pills-tab.nav.nav-pills.nav-justified.mb-3(role='tablist')
            li.nav-item
              a#pills-home-tab.nav-item.nav-link.active(data-toggle='pill', href='#pills-home', role='tab', aria-controls='pills-home', aria-selected='true') Headers
            li.nav-item
              a#pills-profile-tab.nav-item.nav-link(data-toggle='pill', href='#pills-profile', role='tab', aria-controls='pills-profile', aria-selected='false') Raw
            if deviceState
              li.nav-item
                a#pills-profile-tab.nav-item.nav-link(data-toggle='pill', href='#pills-access', role='tab', aria-controls='pills-access', aria-selected='false') Access
          #pills-tabContent.tab-content
            #pills-home.tab-pane.fade.show.active(role='tabpanel', aria-labelledby='pills-home-tab')
              block headers
            #pills-profile.tab-pane.fade(role='tabpanel', aria-labelledby='pills-profile-tab')
              block raw
            if deviceState
              #pills-access.tab-pane.fade(role='tabpanel', aria-labelledby='pills-access-tab')
                block sensitiveInfo
    script(src='https://code.jquery.com/jquery-3.3.1.slim.min.js', integrity='sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo', crossorigin='anonymous')
    script(src='https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js', integrity='sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49', crossorigin='anonymous')
    script(src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous")
    script(src="/js/style.js")